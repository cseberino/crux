(set &
     (func (a b)
           (set result 0)
           (set power  1)
           (while (or a b)
                  (if (and (% a 2) (% b 2))
                      (set result (+ result power))
                      0)
                  (set power (*  power 2))
                  (set a     (>> a     1))
                  (set b     (>> b     1)))
           result))
